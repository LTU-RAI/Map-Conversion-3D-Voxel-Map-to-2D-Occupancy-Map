<launch>
	<node pkg="mapconversion" type="map_conversion" name="map_conversion" respawn="false" output="screen"> 
	    <remap from="ufomap" to="/ufomap_mapping_server_node/map" />
	    <param name="resolution" value="0.1" />
	    <param name="minimum_z" value="1" />
	    <param name="minimum_occupancy" value="10" />
	    <param name="map_frame" value="map"/>
	    <param name="max_slope_ugv" value="0.35" />
	</node>
</launch>
